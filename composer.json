    "keywords": [
        "plex",
        "api",
        "media"
    ],
    "homepage": "https://github.com/jc21/plex-api",
    "license": "BSD-1-Clause",
    "authors": [
        {
            "name": "Jamie Curnow",
            "email": "jc@jc21.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*"
    },
    "require-dev": {
        "victorjonsson/markdowndocs": "dev-master",
        "symfony/dotenv": "6.2.x-dev",
